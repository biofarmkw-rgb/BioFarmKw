            "collapsible_content_item_3ERK7j": {
              "type": "_collapsible-content-item",
              "settings": {
                "heading": "1. How can I track my order?",
                "content": "<p>You’ll receive a tracking number by email once your order is shipped. Use it on our website or the carrier’s site to check your delivery status.</p>",
                "default_expand": false
              },
              "blocks": {}
            },
            "collapsible_content_item_khFgXD": {
              "type": "_collapsible-content-item",
              "settings": {
                "heading": "2. How long does shipping take?",
                "content": "<p>Most orders are delivered within 3–7 business days. International shipping may take longer, depending on location.</p>",
                "default_expand": false
              },
              "blocks": {}
            },
            "collapsible_content_item_wnckna": {
              "type": "_collapsible-content-item",
              "settings": {
                "heading": "3. Do you offer international shipping?",
                "content": "<p>Yes, we ship to many countries worldwide. Shipping costs and delivery times will be shown at checkout.</p>",
