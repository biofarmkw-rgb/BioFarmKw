{%- style -%}
  .section-header {
    position: sticky; /* This is for fixing a Safari z-index issue. PR #2147 */
  }
{%- endstyle -%}

{%- unless section.settings.sticky_header_type == 'none' -%}
  <script src="{{ 'sticky-header.js' | asset_url }}" defer="defer"></script>
{%- endunless -%}

{% liquid
  assign enable_transparent_header = false
  assign enable_transparent_header_index = false
  assign enable_transparent_header_collections = false
  assign enable_transparent_header_collections_list = false
  assign enable_transparent_header_search = false
  assign enable_transparent_header_pages = false

  if settings.enable_transparent_header_index and request.page_type == 'index'
    assign enable_transparent_header_index = true
  endif

  if settings.enable_transparent_header_collections and request.page_type == 'collection'
    assign collection_templates = settings.transparent_collections_templates | replace: ' ', '' | replace: '，', ',' | split: ','
    assign template_suffix = template.suffix

    if collection_templates != empty
      if collection_templates contains template_suffix and template_suffix != blank
        assign enable_transparent_header_collections = true
      elsif collection_templates contains 'default' and template_suffix == blank
        assign enable_transparent_header_collections = true
      endif
    else
      assign enable_transparent_header_collections = true
    endif
  endif

  if settings.enable_transparent_header_collections_list and request.page_type == 'list-collections'
    assign enable_transparent_header_collections_list = true
  endif

  if settings.enable_transparent_header_search and request.page_type == 'search'
    assign enable_transparent_header_search = true
  endif

  if settings.enable_transparent_header_pages and request.page_type == 'page'
    assign page_templates = settings.transparent_pages_templates | replace: ' ', '' | replace: '，', ',' | split: ','
    assign template_suffix = template.suffix

    if page_templates != empty
      if page_templates contains template_suffix and template_suffix != blank
        assign enable_transparent_header_pages = true
      elsif page_templates contains 'default' and template_suffix == blank
        assign enable_transparent_header_pages = true
      endif
    else
      assign enable_transparent_header_pages = true
    endif
  endif

  if enable_transparent_header_index or enable_transparent_header_collections or enable_transparent_header_collections_list or enable_transparent_header_search or enable_transparent_header_pages
    assign enable_transparent_header = true
  endif
%}

<sticky-header
  id="Header-Sticky"
  data-sticky-type="{{ section.settings.sticky_header_type }}"
  class="header-wrapper{% if enable_transparent_header %} header--background-transparent{% endif %}"
>
  <header
    id="Page-Header"
    class="header isolate color-{{ section.settings.color_scheme }} gradient{% if settings.enable_festival_effect %} header-festival-effect festival-{{ settings.festival }}{% endif %}"
    style="--transparent-foreground-color: {{ settings.transparent_foreground_color.red }}, {{ settings.transparent_foreground_color.green }}, {{ settings.transparent_foreground_color.blue }};"
  >
    <div class="header-main page-width logo-position--{{ section.settings.logo_position | default: 'left' }} menu-position--{{ section.settings.menu_position | default: 'logo-line' }}">
      <div class="header-left large-up-hide">
        {% render 'header-drawer',
          blocks: section.blocks,
          category_menu: section.settings.category_menu,
          menu: section.settings.menu,
          default_expand_category: section.settings.default_expand_category,
          show_collection_image: section.settings.show_collection_image,
          show_social: section.settings.show_social_mobile,
          social_style: section.settings.social_style_mobile,
          show_store_button: section.settings.show_store_button,
          show_quick_button: section.settings.show_quick_button,
          button_label: section.settings.button_label,
          button_link: section.settings.button_link
        %}
      </div>
      <div class="header-logo">
        {%- if request.page_type == 'index' -%}
          <h1 class="visually-hidden">{{ shop.name }}</h1>
        {%- endif -%}
        <a href="{{ routes.root_url }}" class="header-logo-link link">
          {%- if section.settings.logo != blank -%}
            <div
              class="header-heading-logo-wrapper{% if enable_transparent_header and settings.transparent_logo != blank %} has-transparent-logo{% endif %}"
              style="--logo-width: {{ section.settings.logo_width }}px; --logo-width-mobile: {{ section.settings.logo_width_mobile }}px;"
            >
              {% capture sizes %}(max-width: {{ section.settings.logo_width | times: 2 }}px) 50vw, {{ section.settings.logo_width }}px{% endcapture %}
              {% capture widths %}{{ section.settings.logo_width }}, {{ section.settings.logo_width | times: 1.5 | round }}, {{ section.settings.logo_width | times: 2 }}{% endcapture %}
              {%- assign logo_alt = section.settings.logo.alt | default: shop.name | escape -%}
              {{
                section.settings.logo
                | image_url: width: 600
                | image_tag:
                  class: 'header-heading-logo motion-reduce',
                  widths: widths,
                  alt: logo_alt,
                  sizes: sizes,
                  height: section.settings.logo.height,
                  width: section.settings.logo.width,
                  loading: 'lazy'
              }}

              {%- assign trans_logo_alt = settings.transparent_logo.alt | default: shop.name | escape -%}
              {%- if settings.transparent_logo != blank -%}
                {{
                  settings.transparent_logo
                  | image_url: width: 600
                  | image_tag:
                    class: 'header-heading-logo transparent-logo motion-reduce',
                    widths: widths,
                    sizes: sizes,
                    alt: trans_logo_alt,
                    height: section.settings.logo.height,
                    width: section.settings.logo.width,
                    loading: 'lazy'
                }}
              {%- endif -%}
            </div>
          {%- else -%}
            <span class="header-logo-text h2">{{ shop.name }}</span>
          {%- endif -%}

          {%- if settings.enable_festival_effect
            and settings.festival == 'christmas'
            and settings.enable_logo_overlay
          -%}
            {% render 'festival-christmas-svgs', svg: 'santa-hat' %}
          {%- endif -%}
        </a>
        {%- if section.settings.show_info_block
          and section.settings.logo_position == 'left'
          or section.settings.menu_position == 'bottom'
        -%}
          <a
            class="header-info link small-hide hover-trigger"
            {% if section.settings.info_link != blank %}
              href="{{ section.settings.info_link }}"
            {% else %}
              role="link"
              aria-disabled="true"
            {% endif %}
          >
            {%- if section.settings.info_icon != 'none' -%}
              {% render 'icon-sets', icon: section.settings.info_icon, class: 'animate--shake' %}
            {%- endif -%}
            <div class="header-info-main">
              <small class="info-subheading light">{{ section.settings.info_sub_heading | escape }}</small>
              <div class="info-heading">{{ section.settings.info_heading | escape }}</div>
            </div>
          </a>
        {%- endif -%}
      </div>
      {%- if section.settings.menu_position == 'logo-line' -%}
        {% render 'header-menu',
          section_id: section.id,
          menu: section.settings.menu,
          blocks: section.blocks,
          enable_hover_open: section.settings.enable_hover_open,
          class: 'small-hide medium-hide'
        %}
      {%- endif -%}
      {%- if section.settings.menu_position == 'bottom' -%}
        <div class="header-search small-hide medium-hide">
          {% render 'search-form',
            input_id: 'Header-Search',
            section_id: section.id,
            in_header: true,
            show_product_types: section.settings.show_product_types,
            color_scheme: section.settings.color_scheme_search
          %}
        </div>
      {%- endif -%}
      <div class="header-right">
        <modal-opener
          class="search-drawer-opener-button{% if section.settings.menu_position == 'bottom' %} large-up-hide{% endif %}"
          role="button"
          aria-haspopup="dialog"
          aria-expanded="false"
          aria-controls="Search-Drawer"
          aria-label="{{ 'templates.search.drawer' | t }}"
        >
          <a
            href="{{ routes.search_url }}"
            class="header-icon icon-wrap fallback-link link"
            aria-label="{{ 'templates.search.search' | t }}"
          >
            {% render 'icon-sets', icon: 'search' %}
          </a>
        </modal-opener>
        <a
          href="{% if customer %}{{ routes.account_url }}{% else %}{{ routes.account_login_url }}{% endif %}"
          class="header-icon icon-wrap link"
          rel="nofollow"
        >
          {% render 'icon-sets', icon: 'account' %}
          <span class="visually-hidden">
            {%- if customer %}{{ 'customer.account.title' | t }}{% else %}{{ 'customer.log_in' | t }}{% endif -%}
          </span>
        </a>
        <modal-opener
          class="cart-drawer-opener"
          {% if settings.cart_type == 'drawer' and request.page_type != 'cart' %}
            role="button"
            aria-label="{{ 'templates.cart.drawer' | t }}"
            aria-haspopup="dialog"
            aria-expanded="false"
            aria-controls="Cart-Drawer"
          {% endif %}
        >
          <a
            id="Cart-Icon-Bubble"
            class="header-icon icon-wrap fallback-link link"
            href="{{ routes.cart_url }}"
            aria-label="{{ 'templates.cart.cart' | t }}"
            dir="ltr"
          >
            {% render 'cart-bubble', style: settings.cart_icon %}
          </a>
        </modal-opener>
      </div>
    </div>
    {%- if section.settings.menu_position == 'bottom' -%}
      <div class="header-nav small-hide">
        <div class="page-width">
          <div class="header-nav-container">
            {%- if section.settings.category_menu != blank -%}
              {% render 'header-menu-categories',
                default_open_in_index: section.settings.default_open_in_index,
                category_menu: section.settings.category_menu,
                blocks: section.blocks,
                show_collection_image: section.settings.show_collection_image,
                enable_hover_open: section.settings.enable_hover_open
              %}
            {%- endif -%}
            {% render 'header-menu',
              section_id: section.id,
              menu: section.settings.menu,
              blocks: section.blocks,
              enable_hover_open: section.settings.enable_hover_open
            %}
            {%- if section.settings.show_quick_button or section.settings.show_store_button -%}
              <div class="header-menu-buttons">
                {%- if section.settings.show_store_button -%}
                  <modal-opener
                    class="header-store-button button button--small button--ethereal"
                    role="button"
                    aria-haspopup="dialog"
                    aria-expanded="false"
                    aria-controls="Stores-Drawer"
                    tabindex="0"
                  >
                    {% render 'icon-sets', icon: 'store' %}
                    {{ 'general.store.find_store' | t }}
                  </modal-opener>
                {%- endif -%}
                {%- if section.settings.show_quick_button -%}
                  <a
                    class="header-quick-button button button--secondary button--small"
                    {% if section.settings.button_link != blank %}
                      href="{{ section.settings.button_link }}"
                    {% else %}
                      role="link"
                      aria-disabled="true"
                    {% endif %}
                  >
                    {{ section.settings.button_label | escape }}
                  </a>
                {%- endif -%}
              </div>
            {%- endif -%}
          </div>
        </div>
      </div>
    {%- endif -%}
  </header>
</sticky-header>

<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "Organization",
    "name": {{ shop.name | json }},
  {% if section.settings.logo %}
      "logo": {{ section.settings.logo | image_url: width: 500 | prepend: "https:" | json }},
    {% endif %}
  "sameAs": [
  {{ settings.social_facebook_link | json }},
  {{ settings.social_instagram_link | json }},
  {{ settings.social_youtube_link | json }},
  {{ settings.social_tiktok_link | json }},
  {{ settings.social_twitter_link | json }},
  {{ settings.social_snapchat_link | json }},
  {{ settings.social_pinterest_link | json }},
  {{ settings.social_tumblr_link | json }},
  {{ settings.social_vimeo_link | json }},
  {{ settings.social_linkedin_link | json }},
  {{ settings.social_whatsapp_link | json }},
  {{ settings.social_reddit_link | json }},
  {{ settings.social_discord_link | json }}
  ],
  "url": {{ request.origin | append: page.url | json }}
  }
</script>

{%- if request.page_type == 'index' -%}
  {% assign potential_action_target = request.origin | append: routes.search_url | append: '?q={search_term_string}' %}
  <script type="application/ld+json">
    {
      "@context": "http://schema.org",
      "@type": "WebSite",
      "name": {{ shop.name | json }},
      "potentialAction": {
        "@type": "SearchAction",
        "target": {{ potential_action_target | json }},
        "query-input": "required name=search_term_string"
      },
      "url": {{ request.origin | append: page.url | json }}
    }
  </script>
{%- endif -%}

